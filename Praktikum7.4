<!DOCTYPE html>
<html>
    <head>
        <title>Praktikum 5.3</title>
        <style>
          header{
            margin: 1em;
          }
          
          .main{
            margin: 1em;
          }
          
          footer{
            margin: 1em;
          }
          
          .box1{
            background-color: aliceblue;
            float: left;
            width: 19%;
            height: 100px;
            margin-right: 1em;
            margin-bottom: 1em;
          }
          
          .box2{
            background-color: lavender;
            float: left;
            width: 79%;
            height: 100px;
            margin-bottom: 1em;
          }
          
          .box3{
            background-color: mistyrose;
            width: 19%;
            height: 500px;
            float: left;
            margin-right: 1em;
            margin-bottom: 1em;
          }
          
          .box4{
            background-color: lavenderblush;
            width: 58%;
            height: 500px;
            float: left;
            margin-right: 1em;
            margin-bottom: 1em;
          }
          
          .box5{
            background-color: mistyrose;
            width: 19%;
            height: 500px;
            float: left;
            margin-bottom: 1em;
          }
          
          .box6{
            background-color: aliceblue;
            width: 100%;
            height: 100px;
            float: left;
          }
        </style>
    </head>
    <body>
      <header>
        <div class="box1"></div>
        <div class="box2"></div>
      </header>
      
      <div class="main">
        <div class="box3"></div>
        <div class="box4"></div>
        <div class="box5"></div>
      </div>
      
      <footer>
        <div class="box6"></div>
      </footer>
    </body>
</html>
